{
  "name": "@xala-technologies/command-center",
  "version": "0.1.0",
  "private": true,
  "description": "GUI for Xala Design Governance Workflows",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "echo '⚠️  ESLint not yet configured for command-center'",
    "format:check": "echo '⚠️  Prettier not yet configured for command-center'",
    "verify:boundaries": "node ../../packages/guardrails/scripts/verify-boundaries.js",
    "verify:design-tokens": "node ../../packages/guardrails/scripts/verify-design-tokens.js",
    "quality": "pnpm typecheck"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@google/generative-ai": "^0.21.0",
    "openai": "^4.52.0",
    "@xala-technologies/design-workflow": "workspace:*",
    "@xala-technologies/platform-ui": "workspace:*",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.22.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.2.1",
    "typescript": "^5.3.3",
    "vite": "^5.2.0"
  }
}
