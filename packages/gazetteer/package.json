{
    "name": "@xala-technologies/gazetteer",
    "version": "0.1.0",
    "description": "Monster Motor - Schema-Driven Page Execution Engine for Xala SaaS Solutions",
    "type": "module",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "sideEffects": false,
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.js",
            "types": "./dist/index.d.ts"
        },
        "./runtime": {
            "import": "./dist/runtime/index.mjs",
            "require": "./dist/runtime/index.js",
            "types": "./dist/runtime/index.d.ts"
        },
        "./hooks": {
            "import": "./dist/hooks/index.mjs",
            "require": "./dist/hooks/index.js",
            "types": "./dist/hooks/index.d.ts"
        },
        "./builder": {
            "import": "./dist/builder/index.mjs",
            "require": "./dist/builder/index.js",
            "types": "./dist/builder/index.d.ts"
        },
        "./ai": {
            "import": "./dist/ai/index.mjs",
            "require": "./dist/ai/index.js",
            "types": "./dist/ai/index.d.ts"
        },
        "./governance": {
            "import": "./dist/governance/index.mjs",
            "require": "./dist/governance/index.js",
            "types": "./dist/governance/index.d.ts"
        },
        "./schema/*": "./schema/*",
        "./catalogs/*": "./catalogs/*"
    },
    "files": [
        "dist",
        "schema",
        "catalogs"
    ],
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "typecheck": "tsc --noEmit",
        "lint": "eslint src --ext .ts,.tsx",
        "test": "vitest run",
        "test:watch": "vitest"
    },
    "peerDependencies": {
        "@xala-technologies/platform-ui": "^2.8.0",
        "react": "^18.0.0",
        "react-dom": "^18.0.0"
    },
    "dependencies": {
        "ajv": "^8.12.0"
    },
    "devDependencies": {
        "@types/react": "^18.2.0",
        "@types/react-dom": "^18.2.0",
        "tsup": "^8.0.0",
        "typescript": "^5.3.0",
        "vitest": "^1.0.0"
    },
    "keywords": [
        "gazetteer",
        "monster-motor",
        "schema-driven",
        "page-registry",
        "xala-platform",
        "declarative-ui"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/xala-technologies/xala-platform-ui.git",
        "directory": "packages/gazetteer"
    },
    "license": "MIT"
}