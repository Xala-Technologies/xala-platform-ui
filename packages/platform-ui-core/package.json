{
    "name": "@xala-technologies/platform-ui-core",
    "version": "1.0.1",
    "description": "Universal UI components built on Norwegian Designsystemet - Core package",
    "type": "module",
    "main": "dist/index.cjs",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "sideEffects": [
        "./dist/styles.js",
        "./dist/styles.cjs"
    ],
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        },
        "./primitives": {
            "types": "./dist/primitives/index.d.ts",
            "import": "./dist/primitives/index.js",
            "require": "./dist/primitives/index.cjs"
        },
        "./composed": {
            "types": "./dist/composed/index.d.ts",
            "import": "./dist/composed/index.js",
            "require": "./dist/composed/index.cjs"
        },
        "./shells": {
            "types": "./dist/shells/index.d.ts",
            "import": "./dist/shells/index.js",
            "require": "./dist/shells/index.cjs"
        },
        "./blocks": {
            "types": "./dist/blocks/index.d.ts",
            "import": "./dist/blocks/index.js",
            "require": "./dist/blocks/index.cjs"
        },
        "./patterns": {
            "types": "./dist/patterns/index.d.ts",
            "import": "./dist/patterns/index.js",
            "require": "./dist/patterns/index.cjs"
        },
        "./themes": {
            "types": "./dist/themes/index.d.ts",
            "import": "./dist/themes/index.js",
            "require": "./dist/themes/index.cjs"
        },
        "./tokens": {
            "types": "./dist/tokens/index.d.ts",
            "import": "./dist/tokens/index.js",
            "require": "./dist/tokens/index.cjs"
        },
        "./types": {
            "types": "./dist/types/index.d.ts",
            "import": "./dist/types/index.js",
            "require": "./dist/types/index.cjs"
        },
        "./styles": {
            "types": "./dist/styles.d.ts",
            "import": "./dist/styles.js",
            "require": "./dist/styles.cjs"
        }
    },
    "typesVersions": {
        "*": {
            "primitives": [
                "./dist/primitives/index.d.ts"
            ],
            "composed": [
                "./dist/composed/index.d.ts"
            ],
            "shells": [
                "./dist/shells/index.d.ts"
            ],
            "blocks": [
                "./dist/blocks/index.d.ts"
            ],
            "patterns": [
                "./dist/patterns/index.d.ts"
            ],
            "themes": [
                "./dist/themes/index.d.ts"
            ],
            "tokens": [
                "./dist/tokens/index.d.ts"
            ],
            "types": [
                "./dist/types/index.d.ts"
            ],
            "styles": [
                "./dist/styles.d.ts"
            ]
        }
    },
    "files": [
        "dist",
        "src"
    ],
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "clean": "rm -rf dist",
        "typecheck": "tsc --noEmit",
        "lint": "eslint src --ext .ts,.tsx",
        "lint:fix": "eslint src --ext .ts,.tsx --fix"
    },
    "keywords": [
        "xala",
        "xalatechnologies",
        "ui",
        "react",
        "typescript",
        "designsystemet",
        "design-system",
        "components",
        "core"
    ],
    "author": "Xala Technologies",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Xala-Technologies/xala-platform-ui.git",
        "directory": "packages/platform-ui-core"
    },
    "publishConfig": {
        "registry": "https://npm.pkg.github.com",
        "access": "public"
    },
    "dependencies": {
        "@digdir/designsystemet-css": "^1.11.0",
        "@digdir/designsystemet-react": "^1.9.0",
        "@fontsource/inter": "^5.2.8",
        "@navikt/aksel-icons": "^8.0.1",
        "lucide-react": "^0.562.0",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^8.0.0",
        "@typescript-eslint/parser": "^8.0.0",
        "eslint": "^9.0.0",
        "eslint-plugin-react": "^7.37.0",
        "eslint-plugin-react-hooks": "^5.1.0",
        "prettier": "^3.2.5",
        "tsup": "^8.0.1",
        "typescript": "^5.3.3"
    },
    "peerDependencies": {
        "react": ">=18.0.0",
        "react-dom": ">=18.0.0",
        "react-router-dom": ">=6.0.0"
    },
    "peerDependenciesMeta": {
        "react": {
            "optional": false
        },
        "react-dom": {
            "optional": false
        },
        "react-router-dom": {
            "optional": true
        }
    }
}