{
  "name": "@xala-technologies/design-workflow",
  "version": "0.1.0",
  "description": "AI-guided design workflow for Xala Platform UI",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./adapters": {
      "types": "./dist/adapters/index.d.ts",
      "import": "./dist/adapters/index.js"
    },
    "./generators": {
      "types": "./dist/generators/index.d.ts",
      "import": "./dist/generators/index.js"
    },
    "./schemas": {
      "types": "./dist/schemas/index.d.ts",
      "import": "./dist/schemas/index.js"
    },
    "./commands/*": "./commands/*.md",
    "./prompts/*": "./prompts/*.md"
  },
  "bin": {
    "design-workflow": "./bin/cli.js"
  },
  "files": [
    "dist",
    "bin",
    "commands",
    "prompts",
    "claude"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm build"
  },
  "peerDependencies": {},
  "peerDependenciesMeta": {},
  "devDependencies": {
    "@types/node": "^20.10.0",
    "eslint": "^9.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "zod": "^3.23.0",
    "chalk": "^5.3.0",
    "yaml": "^2.3.4"
  },
  "keywords": [
    "design-system",
    "workflow",
    "ai",
    "claude",
    "xala",
    "designsystemet"
  ],
  "author": "Xala Technologies",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Xala-Technologies/xala-platform-ui.git",
    "directory": "packages/design-workflow"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "public"
  }
}
