{
    "name": "@xala-technologies/platform-ui-digilist",
    "version": "1.0.1",
    "description": "Digilist-specific UI components and features - Extension package",
    "type": "module",
    "main": "dist/index.cjs",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        },
        "./blocks": {
            "types": "./dist/blocks/index.d.ts",
            "import": "./dist/blocks/index.js",
            "require": "./dist/blocks/index.cjs"
        },
        "./patterns": {
            "types": "./dist/patterns/index.d.ts",
            "import": "./dist/patterns/index.js",
            "require": "./dist/patterns/index.cjs"
        },
        "./features/booking": {
            "types": "./dist/features/booking.d.ts",
            "import": "./dist/features/booking.js",
            "default": "./dist/features/booking.js"
        },
        "./features/calendar": {
            "types": "./dist/features/calendar.d.ts",
            "import": "./dist/features/calendar.js",
            "default": "./dist/features/calendar.js"
        },
        "./features/seasons": {
            "types": "./dist/features/seasons.d.ts",
            "import": "./dist/features/seasons.js",
            "default": "./dist/features/seasons.js"
        },
        "./features/rental-objects": {
            "types": "./dist/features/rental-objects.d.ts",
            "import": "./dist/features/rental-objects.js",
            "default": "./dist/features/rental-objects.js"
        },
        "./features/rental-object-details": {
            "types": "./dist/features/rental-object-details.d.ts",
            "import": "./dist/features/rental-object-details.js",
            "default": "./dist/features/rental-object-details.js"
        },
        "./features/reviews": {
            "types": "./dist/features/reviews.d.ts",
            "import": "./dist/features/reviews.js",
            "default": "./dist/features/reviews.js"
        },
        "./features/gdpr": {
            "types": "./dist/features/gdpr.d.ts",
            "import": "./dist/features/gdpr.js",
            "default": "./dist/features/gdpr.js"
        },
        "./features/settings": {
            "types": "./dist/features/settings.d.ts",
            "import": "./dist/features/settings.js",
            "default": "./dist/features/settings.js"
        },
        "./features/notifications": {
            "types": "./dist/features/notifications.d.ts",
            "import": "./dist/features/notifications.js",
            "default": "./dist/features/notifications.js"
        },
        "./features/notification-reports": {
            "types": "./dist/features/notification-reports.d.ts",
            "import": "./dist/features/notification-reports.js",
            "default": "./dist/features/notification-reports.js"
        },
        "./features/organizations": {
            "types": "./dist/features/organizations.d.ts",
            "import": "./dist/features/organizations.js",
            "default": "./dist/features/organizations.js"
        },
        "./features/docs": {
            "types": "./dist/features/docs.d.ts",
            "import": "./dist/features/docs.js",
            "default": "./dist/features/docs.js"
        },
        "./types": {
            "types": "./dist/types/index.d.ts",
            "import": "./dist/types/index.js",
            "require": "./dist/types/index.cjs"
        }
    },
    "typesVersions": {
        "*": {
            "blocks": [
                "./dist/blocks/index.d.ts"
            ],
            "patterns": [
                "./dist/patterns/index.d.ts"
            ],
            "types": [
                "./dist/types/index.d.ts"
            ]
        }
    },
    "files": [
        "dist",
        "src"
    ],
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "clean": "rm -rf dist",
        "typecheck": "tsc --noEmit",
        "lint": "eslint src --ext .ts,.tsx",
        "lint:fix": "eslint src --ext .ts,.tsx --fix"
    },
    "keywords": [
        "xala",
        "xalatechnologies",
        "ui",
        "react",
        "typescript",
        "digilist",
        "booking",
        "rental"
    ],
    "author": "Xala Technologies",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Xala-Technologies/xala-platform-ui.git",
        "directory": "packages/platform-ui-digilist"
    },
    "publishConfig": {
        "registry": "https://npm.pkg.github.com",
        "access": "public"
    },
    "dependencies": {
        "@xala-technologies/platform-ui-core": "^1.0.0"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^8.0.0",
        "@typescript-eslint/parser": "^8.0.0",
        "eslint": "^9.0.0",
        "eslint-plugin-react": "^7.37.0",
        "eslint-plugin-react-hooks": "^5.1.0",
        "lucide-react": "^0.562.0",
        "prettier": "^3.2.5",
        "tsup": "^8.0.1",
        "typescript": "^5.3.3"
    },
    "peerDependencies": {
        "lucide-react": ">=0.400.0",
        "react": ">=18.0.0",
        "react-dom": ">=18.0.0",
        "react-router-dom": ">=6.0.0"
    },
    "peerDependenciesMeta": {
        "react": {
            "optional": false
        },
        "react-dom": {
            "optional": false
        },
        "react-router-dom": {
            "optional": true
        }
    }
}
